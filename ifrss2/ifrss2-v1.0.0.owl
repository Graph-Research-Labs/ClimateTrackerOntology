    <!-- http://www.graphresearchlabs.com/SustainabilityFinancialDisclosure/EnterpriseValue -->

    <owl:Class rdf:about="http://www.graphresearchlabs.com/SustainabilityFinancialDisclosure/EnterpriseValue">
